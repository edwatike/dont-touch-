# Настройки прокси
PROXY_LIST=http://user:pass@host:port,http://user:pass@host:port

# Настройки задержек (в секундах)
DELAY_MIN=2
DELAY_MAX=5

# Настройки парсинга
MAX_PAGES=10
RETRY_COUNT=3
TIMEOUT=30

# Настройки потоков
NUM_THREADS=4

# Настройки логирования
LOG_LEVEL=INFO
SAVE_DEBUG_SCREENSHOTS=false

# Настройки базы данных
DB_PATH=parser_results.db 